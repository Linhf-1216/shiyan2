    <color name="colorPrimaryDark">#303F9F</color> <!-- 状态栏颜色（深蓝色） -->
    <color name="colorAccent">#FF4081</color>      <!-- 控件高亮色（粉色） -->

    <!-- 可保留文件中原有颜色（如白色、黑色），无则无需添加 -->
    <color name="white">#FFFFFF</color>
    <color name="black">#000000</color>
</resources>